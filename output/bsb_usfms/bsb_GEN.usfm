 prison  |strong="1004" srcloc="WLC:GEN.39.20.16" \w* \w  . . . ,  |strong="5470" srcloc="WLC:GEN.39.20.17" \w* \v 21 \w  the LORD  |strong="3068" srcloc="WLC:GEN.39.21.2" \w* \w  was  |strong="1961" srcloc="WLC:GEN.39.21.1" \w* \w  with  |strong="854" srcloc="WLC:GEN.39.21.3" \w* \w  [him]  |strong="3130" srcloc="WLC:GEN.39.21.4" \w* \w  and extended  |strong="5186" srcloc="WLC:GEN.39.21.5" \w* \w  kindness  |strong="2617" srcloc="WLC:GEN.39.21.7" \w* \w  to him ,  |strong="413" srcloc="WLC:GEN.39.21.6" \w* \w  granting him  |strong="5414" srcloc="WLC:GEN.39.21.8" \w* \w  favor  |strong="2580" srcloc="WLC:GEN.39.21.9" \w* \w  in the eyes  |strong="5869" srcloc="WLC:GEN.39.21.10" \w* \w  of the prison  |strong="1004" srcloc="WLC:GEN.39.21.12" \w* \w  . . .  |strong="5470" srcloc="WLC:GEN.39.21.13" \w* \w  warden .  |strong="8269" srcloc="WLC:GEN.39.21.11" \w* \v 22 \w  And the warden  |strong="8269" srcloc="WLC:GEN.39.22.2" \w* \w  . . .  |strong="1004" srcloc="WLC:GEN.39.22.3" \w* \w  . . .  |strong="5470" srcloc="WLC:GEN.39.22.4" \w* \w  put  |strong="5414" srcloc="WLC:GEN.39.22.1" \w* \w  all  |strong="3605" srcloc="WLC:GEN.39.22.8" \w* \w  the prisoners  |strong="615" srcloc="WLC:GEN.39.22.9" \w* \w  . . .  |strong="834" srcloc="WLC:GEN.39.22.10" \w* \w  -  |strong="853" srcloc="WLC:GEN.39.22.13" \w* \w  under Joseph’s  |strong="3130" srcloc="WLC:GEN.39.22.6" \w* \w  -  |strong="853" srcloc="WLC:GEN.39.22.7" \w* \w  care ,  |strong="3027" srcloc="WLC:GEN.39.22.5" \w* \w  so that he  |strong="1931" srcloc="WLC:GEN.39.22.18" \w* \w  was  |strong="1961" srcloc="WLC:GEN.39.22.19" \w* \w  responsible  |strong="6213" srcloc="WLC:GEN.39.22.16" \w* \w  for all  |strong="3605" srcloc="WLC:GEN.39.22.14" \w* \w  that  |strong="834" srcloc="WLC:GEN.39.22.15" \w* \w  was done  |strong="6213" srcloc="WLC:GEN.39.22.20" \w* \w  -  |strong="8033" srcloc="WLC:GEN.39.22.17" \w* \w  in  |strong="1004" srcloc="WLC:GEN.39.22.11" \w* \w  the prison .  |strong="5470" srcloc="WLC:GEN.39