* \w  days ,  |strong="3117" srcloc="WLC:DAN.1.15.-6" \w* \w  they looked  |strong="7200" srcloc="WLC:DAN.1.15.-4" \w* \w  . . .  |strong="4758" srcloc="WLC:DAN.1.15.-3" \w* \w  healthier  |strong="2896" srcloc="WLC:DAN.1.15.-2" \w* \w  and better nourished  |strong="1277" srcloc="WLC:DAN.1.15.-1" \w* \w  . . .  |strong="1320" srcloc="WLC:DAN.1.15.0" \w* \w  than all  |strong="3605" srcloc="WLC:DAN.1.15.2" \w* \w  the young men  |strong="3206" srcloc="WLC:DAN.1.15.3" \w* \w  who were eating  |strong="398" srcloc="WLC:DAN.1.15.4" \w* \w  -  |strong="853" srcloc="WLC:DAN.1.15.5" \w* \w  the king’s  |strong="4428" srcloc="WLC:DAN.1.15.7" \w* \w  food .  |strong="6598" srcloc="WLC:DAN.1.15.6" \w* \v 16 \w  So  |strong="1961" srcloc="WLC:DAN.1.16.1" \w* \w  the steward  |strong="4453" srcloc="WLC:DAN.1.16.2" \w* \w  continued to withhold  |strong="5375" srcloc="WLC:DAN.1.16.3" \w* \w  -  |strong="853" srcloc="WLC:DAN.1.16.4" \w* \w  their choice food  |strong="6598" srcloc="WLC:DAN.1.16.5" \w* \w  and the wine  |strong="3196" srcloc="WLC:DAN.1.16.6" \w* \w  they were to drink ,  |strong="4960" srcloc="WLC:DAN.1.16.7" \w* \w  and he gave  |strong="5414" srcloc="WLC:DAN.1.16.8" \w* \w  them  |srcloc="WLC:DAN.1.16.9" \w* \w  vegetables [instead] .  |strong="2235" srcloc="WLC:DAN.1.16.10" \w* 
