4" srcloc="WLC:PSA.8.8.5" \w* \w  the paths  |strong="734" srcloc="WLC:PSA.8.8.6" \w* \w  of the seas .  |strong="3220" srcloc="WLC:PSA.8.8.7" \w* \v 9 \w  O LORD ,  |strong="3068" srcloc="WLC:PSA.8.9.1" \w* \w  our Lord ,  |strong="113" srcloc="WLC:PSA.8.9.2" \w* \w  how  |strong="4100" srcloc="WLC:PSA.8.9.3" \w* \w  majestic  |strong="117" srcloc="WLC:PSA.8.9.4" \w* \w  is Your name  |strong="8034" srcloc="WLC:PSA.8.9.5" \w* \w  in all  |strong="3605" srcloc="WLC:PSA.8.9.6" \w* \w  the earth !  |strong="776" srcloc="WLC:PSA.8.9.7" \w* 
